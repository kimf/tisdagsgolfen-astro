{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "_legacy"
  ],
  "compilerOptions": {
    "types": [
      "astro/client"
    ],
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "plugins": [
    {
      "name": "@astrojs/ts-plugin"
    }
  ]
}